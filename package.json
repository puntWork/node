{
  "name": "@punt/node",
  "version": "1.1.0",
  "description": "Punt is a powerful, developer friendly queue for processing background jobs.",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --forceExit --runInBand",
    "test:watch": "jest --watch --forceExit --runInBand",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puntWork/node.git"
  },
  "keywords": [
    "background",
    "jobs",
    "workers",
    "pubsub",
    "redis",
    "streams",
    "queue"
  ],
  "author": "Andre Oliveira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/puntWork/node/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "punt": "./bin/cli"
  },
  "files": [
    "dist/**/*",
    "bin/*"
  ],
  "homepage": "https://github.com/puntWork/node#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.22.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "debug": "^4.3.4",
    "ioredis": "^4.25.0"
  }
}
